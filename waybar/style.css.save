*{
  font-size: 13px;
  font-family: "IBM Plex Sans Medium";
  color: #dadada;
}

#waybar {
  background: transparent;
  border-radius: 5px;
}

#workspaces {
  border-radius:5px;
  box-shadow:0px 0px black;
}

#custom-telegram,
#workspaces,
#backlight,
#wireplumber,
#clock,
#custom-power,
#custom-notification {
  margin-left:5px;
  margin-right: 5px;
  background: #141b1e;
  border-radius:5px 0px 0px 5px;
  padding-left:10px;
  padding-right: 10px;
}

#wireplumber,
#backlight {
  margin:0px;
}

#backlight {
  border-radius:0px;
}

#wireplumber {
  border-radius: 0px 5px 5px 0px;
}

#custom-calendar,
#custom-telegram,
#clock,
#workspaces,
#custom-power,
#custom-notification {
  border-radius: 5px;
}

#custom-power{
  color:#c47fd5;
  margin-right:10px;
  padding-right:14px;
}
